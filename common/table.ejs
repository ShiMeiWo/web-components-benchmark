<tr>
    <th scope="row" class="element-infos">
        <%= library.name %><br />
        <span class="light"> <%= library.version %></span>
        <div class="element-infos__stars">
            <%= library.stars %>&nbsp;<ion-icon name="star"></ion-icon>
        </div>
        <div>
            <a href="./demos/todomvc/<%= library.slug %>" target="_blank"
                >Demo</a
            >
        </div>
    </th>
    <td class="align-middle">
        <!--<div class="progress position-relative">
            <div class="progress-bar bg-1" role="progressbar" style="width: <%= Math.floor((library.size / todo.max.size) * 100)%>%">
                <%= library.size %>
            </div>
        </div>-->
        <div class="progress position-relative">
            <div
                class="progress-bar bg-1"
                role="progressbar"
                style="width: <%= Math.floor((library.size / todo.max.size) * 100)%>%"
            ></div>
            <span class="justify-content-center d-flex position-absolute w-100"
                ><%= library.size %></span
            >
        </div>
    </td>
    <td class="align-middle">
        <div class="progress position-relative">
            <div
                class="progress-bar bg-2"
                role="progressbar"
                style="width: <%= Math.floor((library.load / todo.max.load) * 100)%>%"
            >
                <span
                    class="justify-content-center d-flex position-absolute w-100"
                >
                    <%= library.load %>
                </span>
            </div>
        </div>
    </td>
    <td class="align-middle">
        <div class="progress position-relative">
            <div
                class="progress-bar bg-3"
                role="progressbar"
                style="width: <%= Math.floor((library.tti / todo.max.tti) * 100)%>%"
            >
                <span
                    class="justify-content-center d-flex position-absolute w-100"
                >
                    <%= library.tti %>
                </span>
            </div>
        </div>
    </td>
    <td class="align-middle">
        <div class="progress position-relative">
            <div
                class="progress-bar bg-4"
                role="progressbar"
                style="width: <%= Math.floor((library.create / todo.max.create) * 100)%>%"
            >
                <span
                    class="justify-content-center d-flex position-absolute w-100"
                >
                    <%= library.create %>
                </span>
            </div>
        </div>
    </td>
    <td class="align-middle">
        <div class="progress position-relative">
            <div
                class="progress-bar bg-5"
                role="progressbar"
                style="width: <%= Math.floor((library.delete / todo.max.delete) * 100)%>%"
            >
                <span
                    class="justify-content-center d-flex position-absolute w-100"
                >
                    <%= library.delete %>
                </span>
            </div>
        </div>
    </td>
    <td class="align-middle">
        <div class="progress position-relative">
            <div
                class="progress-bar bg-6"
                role="progressbar"
                style="width: <%= Math.floor((library.edit / todo.max.edit) * 100)%>%"
            >
                <span
                    class="justify-content-center d-flex position-absolute w-100"
                >
                    <%= library.edit %>
                </span>
            </div>
        </div>
    </td>
</tr>
